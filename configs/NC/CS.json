{
    "task": "NC",
    "dataset": "CS",
    "root_path": "./datasets",
    "val_every": 5,
    "exp_iters": 10,
    "log_dir": "./logs/",
    "result_dir": "./results/",
    "task_model_path": "NC_CS_model.pt",
    "checkpoints": "./checkpoints/",
    "add_self_loop": false,
    "n_layers": 3,
    "hid_dim": 512,
    "embed_dim": 512,
    "dropout": 0.2,
    "act": "gelu",
    "input_act": "gelu",
    "norm": "ln",
    "bias": true,
    "tau": 1.0,
    "layer_wise": true,
    "lr_nc": 3e-05,
    "weight_decay_nc": 0,
    "epochs_nc": 2000,
    "patience_nc": 15,
    "use_gpu": true,
    "gpu": 0,
    "devices": "0,1"
}