{
    "task": "Transfer",
    "dataset": "line",
    "root_path": "./datasets",
    "val_every": 10,
    "exp_iters": 10,
    "log_dir": "./results/logs",
    "result_dir": "./results/",
    "task_model_path": "Transfer_line_model.pt",
    "checkpoints": "./checkpoints/",
    "add_self_loop": false,
    "n_layers": 5,
    "hid_dim": 64,
    "embed_dim": 64,
    "dropout": 0.0,
    "act": "tanh",
    "input_act": "tanh",
    "norm": "bn",
    "bias": true,
    "lr_nc": 3e-05,
    "weight_decay_nc": 0,
    "epochs_nc": 2000,
    "patience_nc": 100,
    "additional_layers": 0,
    "distance_list": [
        50,
        10,
        5,
        3
    ],
    "lr_trans": 0.001,
    "weight_decay_trans": 0,
    "epochs_trans": 2000,
    "patience_trans": 10,
    "use_gpu": true,
    "gpu": 0,
    "devices": "0,1"
}